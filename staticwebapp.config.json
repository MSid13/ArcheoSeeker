{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif,svg}", "/css/*", "/js/*", "/*.js", "/*.css"]
  },
  "platform": {
    "apiRuntime": "node:18"
  },
  "trailingSlash": "never"
}